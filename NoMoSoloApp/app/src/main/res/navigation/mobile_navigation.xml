<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/navigation_profile">

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.example.nomosoloapp.ui.profile.ProfileFragment"
        android:label="@string/navprofile"
        tools:layout="@layout/fragment_profile" />
    <fragment
        android:id="@+id/navigation_match"
        android:name="com.example.nomosoloapp.ui.match.MatchFragment"
        android:label="@string/navmatch"
        tools:layout="@layout/fragment_match" />
    <fragment
        android:id="@+id/navigation_chat"
        android:name="com.example.nomosoloapp.ui.chat.ChatFragment"
        android:label="@string/navchat"
        tools:layout="@layout/fragment_chat" />
    <fragment
        android:id="@+id/navigation_calendar"
        android:name="com.example.nomosoloapp.ui.calendar.CalendarFragment"
        android:label="@string/navcalendar"
        tools:layout="@layout/fragment_calendar" />

    <fragment
        android:id="@+id/navigation_match_profile"
        android:name="com.example.nomosoloapp.ui.match.MatchProfile"
        android:label="@string/navmatch"
        tools:layout="@layout/fragment_match_profile" />
    <fragment
        android:id="@+id/navigation_edit_profile"
        android:name="com.example.nomosoloapp.ui.profile.EditProfileFragment"
        android:label="@string/navprofile"
        tools:layout="@layout/fragment_edit_profile" />
    <fragment
        android:id="@+id/navigation_calendar_notes_view"
        android:name="com.example.nomosoloapp.ui.calendar.CalendarNotesView"
        android:label="Notes"
        tools:layout="@layout/fragment_calendar_notes_view" />
    <fragment
        android:id="@+id/navigation_chat_messages"
        android:name="com.example.nomosoloapp.ui.chat.ChatMessages"
        android:label="Chat"
        tools:layout="@layout/fragment_chat_messages" />
</navigation>